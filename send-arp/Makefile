####### Compile

arphdr.o: arphdr.cpp arphdr.h \
		mac.h \
		ip.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o arphdr.o arphdr.cpp

ethhdr.o: ethhdr.cpp ethhdr.h \
		mac.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o ethhdr.o ethhdr.cpp

ip.o: ip.cpp ip.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o ip.o ip.cpp

iphdr.o: iphdr.cpp iphdr.h \
		ip.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o iphdr.o iphdr.cpp

mac.o: mac.cpp mac.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o mac.o mac.cpp

main.o: main.cpp networkcontroller.h \
		mac.h \
		ethhdr.h \
		arphdr.h \
		ip.h \
		iphdr.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o main.cpp

networkcontroller.o: networkcontroller.cpp networkcontroller.h \
		mac.h \
		ethhdr.h \
		arphdr.h \
		ip.h \
		iphdr.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o networkcontroller.o networkcontroller.cpp

moc_networkcontroller.o: moc_networkcontroller.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_networkcontroller.o moc_networkcontroller.cpp

####### Install

install_target: first FORCE
	@test -d $(INSTALL_ROOT)/opt/send-arp/bin || mkdir -p $(INSTALL_ROOT)/opt/send-arp/bin
	$(QINSTALL_PROGRAM) $(QMAKE_TARGET) $(INSTALL_ROOT)/opt/send-arp/bin/$(QMAKE_TARGET)

uninstall_target: FORCE
	-$(DEL_FILE) $(INSTALL_ROOT)/opt/send-arp/bin/$(QMAKE_TARGET)
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/send-arp/bin/ 


install: install_target  FORCE

uninstall: uninstall_target  FORCE

FORCE:

.SUFFIXES:

